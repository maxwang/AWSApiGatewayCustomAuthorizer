{
  "exclude": [
    "**/bin",
    "**/bower_components",
    "**/jspm_packages",
    "**/node_modules",
    "**/obj",
    "**/platforms"
  ],
  "AWS": {
    "Region": "ap-southeast-2"
  },
    "Lambda.Logging": {
    "IncludeCategory": true,
    "IncludeLogLevel": true,
    "IncludeNewline": true,
    "IncludeException": true,
    "IncludeEventId": true,
    "IncludeScopes": true,
    "LogLevel": {
      "Default": "Debug",
      "Microsoft": "Debug"
    }
  }
}